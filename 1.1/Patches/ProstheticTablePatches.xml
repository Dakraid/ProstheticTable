<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationRemove">
		<xpath>Defs/ThingDef[@Name = "BodyPartProstheticMakeableBase"]/recipeMaker/recipeUsers</xpath>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[@Name = "BodyPartProstheticMakeableBase"]/recipeMaker</xpath>
		<value>
		  <recipeUsers>
				<li>TableSyrProsthetics</li>
		  </recipeUsers>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>Defs/ThingDef[@Name = "BodyPartBionicBase"]/recipeMaker/recipeUsers</xpath>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[@Name = "BodyPartBionicBase"]/recipeMaker</xpath>
		<value>
		  <recipeUsers Inherit="False">
				<li>TableSyrProsthetics</li>
		  </recipeUsers>
		</value>
	</Operation>

	<!-- EPOE -->
	<Operation Class="PatchOperationSequence">
    <success>Always</success>
	
	  <operations>
		<li Class="PatchOperationTest">
			<xpath>Defs/RecipeDef[@Name = "RecipeBionicProsthesis"]</xpath>
		</li>
	
		<li Class="PatchOperationRemove">
			<xpath>Defs/RecipeDef[@Name = "RecipeBionicProsthesis" or @Name = "RecipeSimpleProsthesis" or @Name = "RecipeAdvancedProsthesis"]/recipeUsers</xpath>
		</li>
		
		<li Class="PatchOperationAdd">
			<xpath>Defs/RecipeDef[@Name = "RecipeBionicProsthesis" or @Name = "RecipeSimpleProsthesis" or @Name = "RecipeAdvancedProsthesis"]</xpath>
			<value>
			  <recipeUsers>
				<li>TableSyrProsthetics</li>
			  </recipeUsers>
			</value>
		</li>
	  </operations>
	</Operation>
	
	<!-- Orassans -->
	<Operation Class="PatchOperationSequence">
    <success>Always</success>
	
	  <operations>
		<li Class="PatchOperationTest">
			<xpath>Defs/ThingDef[defName = "BionicTail"]</xpath>
		</li>
	
		<li Class="PatchOperationRemove">
			<xpath>*/ThingDef[defName = "BionicTail"]/recipeMaker/recipeUsers</xpath>
		</li>
		
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "BionicTail"]/recipeMaker</xpath>
			<value>
			  <recipeUsers Inherit="False">
				<li>TableSyrProsthetics</li>
			  </recipeUsers>
			</value>
		</li>
	  </operations>
	</Operation>


</Patch>